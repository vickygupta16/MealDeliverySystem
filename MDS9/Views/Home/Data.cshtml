@model IEnumerable<object>
@{
    List<MDS9.Models.UserProfile> lstups = Model.ToList()[0] as List<MDS9.Models.UserProfile>;
    List<MDS9.Models.Meal> lstm = Model.ToList()[1] as List<MDS9.Models.Meal>;
    List<MDS9.Models.OrderDetails> lstod = Model.ToList()[2] as List<MDS9.Models.OrderDetails>;
    List<MDS9.Models.Student> lstst = Model.ToList()[3] as List<MDS9.Models.Student>;
    List<MDS9.Models.Bill> lstb = Model.ToList()[4] as List<MDS9.Models.Bill>;
}
<html>
    <head>
        <title>Data</title>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        <link rel="shortcut icon" type="image/x-icon" href="~/Content/themes/base/images/Logo1.jpg"/>
        <style>
            #sidebar {
              width: 200px;
              background: #2c3e50;
              position: absolute;
              height: 100%;
              top: 0;
              left: -200px;
              transition: left 0.5s ease;
            }
            #sidebar.visible {
              left: 0px;
              transition: left 0.7s ease;
            }
            .ul2 {
              margin: 0;
              padding: 0;
            }
            .li2 {
              list-style: none;
              padding: 10px 0 10px 10px;
              background: #46637F;
              border-bottom: 1px solid #233140;
            }
            .li2 a {
              text-decoration: none;
              color: white;
            }
            #sidebar-btn {
              display: inline-block;
              vertical-align: middle;
              width: 20px;
              height: 15px;
              cursor: pointer;
              margin: 20px;
              position: absolute;
              right: -60px;
              top: 0px;
            }
            #sidebar-btn span {
              background-color: yellow;
              height: 2px;
              display: block;
              margin-bottom: 5px;
            }
            #sidebar-btn span:nth-child(2) {
              width: 75%;
            }
            .ul1{
                font-size: 20px;
                color:blue;
                padding-left:40px;
            }
            .li1 {
                color: blue;
                display:inline;
            }
            .li1 a{
                color: blue;
                text-decoration:none;
            }
            .mysec {
                font-size:18px;
            }
            .nav-item {
                padding:10px;
            }
            .mysec a:hover{
                text-decoration:none;
                background-color:yellow;
                padding:5px;
                border-radius:5px;
            }
        </style>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
        <script>
        $(document).ready(function () {
            $('#sidebar-btn').on('click', function () {
                $('#sidebar').toggleClass('visible');
            });
        });
        </script>
    </head>
    <body>
        
        <header style="background-color:black;padding:10px">
                <div style="text-align:center">
                    <section class="mysec">
                        <ul class="nav justify-content-center">
                          <li class="nav-item">
                            @Html.ActionLink("Home", "Index", "Home")
                          </li>
                          <li class="nav-item">
                            @Html.ActionLink("About", "About", "Home")
                          </li>
                          <li class="nav-item">
                            @Html.ActionLink("Contact", "Contact", "Home")
                          </li>
                          <li class="nav-item">
                              @{Html.RenderPartial("_LoginPartial");}
                          </li>
                        </ul>
                    </section>
                </div>
        </header>   

        <div id="sidebar">
            <ul class="ul2">
              <li class="li2">@Html.ActionLink("Home","Index", "Home")</li>
              <li class="li2">@Html.ActionLink("Add Meal","Create","Meal")</li>
              <li class="li2">@Html.ActionLink("Meal List","Index","Meal")</li>
              <li class="li2">@Html.ActionLink("Autogenerated Bill List","BillList","Bill")</li>
              <li class="li2">@Html.ActionLink("Generate Bill","Create","Bill")</li>
              <li class="li2">@Html.ActionLink("Add Student","Create","Student")</li>
              <li class="li2">@Html.ActionLink("Student List","Index","Student")</li>
            </ul>
            <div id="sidebar-btn">
                <span></span>
                <span></span>
                <span></span>
            </div>
        </div>
        

        <div class="card" style="width:800px;margin:auto;margin-top:30px;
            box-shadow:10px 10px 5px grey" align="center">
            <h5 class="card-header" style="background-color:lightcoral">Parent Account</h5>
            <div class="card-body" style="background-color:lightpink">
                <table cellpadding="2px" align="center" style="text-align:center;"
                    rules="rows" frame="hsides">
                    <tr>
                        <th>Parent ID</th>
                        <th>Email ID</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Gender</th>
                        <th>Number of Children</th>
                        <th>Address</th>
                    </tr>
                    @foreach (var item in lstups)
                    {
                        <tr>
                            <td>@item.UserId</td>
                            <td>@item.UserName</td>
                            <td>@item.FName</td>
                            <td>@item.LName</td>
                            <td>@item.Gender</td>
                            <td>@item.NOC</td>
                            <td>@item.Address</td>
                        </tr>
                    }
                </table>
            </div>
        </div>


        <div class="card" style="width:800px;margin:auto;margin-top:30px;
            box-shadow:10px 10px 5px grey" align="center">
            <h5 class="card-header" style="background-color:#29A10E">Student Data</h5>
            <div class="card-body" style="background-color:#8DF077">
                <table cellpadding="2px" align="center" style="text-align:center;"
                    rules="rows" frame="hsides">
                    <tr>
                        <th>Student ID</th>
                        <th>Parent ID</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Class</th>
                        <th>Division</th>
                        <th>Roll Number</th>
                        <th>Age</th>
                    </tr>
                    
                </table>
            </div>
        </div>



        <div class="card" style="width:800px;margin:auto;margin-top:30px;
            box-shadow:10px 10px 5px grey" align="center">
            <h5 class="card-header" style="background-color:#B9770E">Meal Data</h5>
            <div class="card-body" style="background-color:#F8C471">
                <table cellpadding="2px" align="center" style="text-align:center;"
                    rules="rows" frame="hsides">
                    <tr>
                        <th>Meal ID</th>
                        <th>Name</th>
                        <th>Description</th>
                        <th>Price</th>
                    </tr>
                    @foreach (var item in lstm)
                    {
                        <tr>
                            <td>@item.MealId</td>
                            <td>@item.Name</td>
                            <td>@item.Description</td>
                            <td>@item.Price</td>
                        </tr>
                    }
                </table>
            </div>
        </div>



        <div class="card" style="width:800px;margin:auto;margin-top:30px;
            box-shadow:10px 10px 5px grey" align="center">
            <h5 class="card-header" style="background-color:#1F618D">Meal Order Data</h5>
            <div class="card-body" style="background-color:#7FB3D5">
                <table cellpadding="2px" align="center" style="text-align:center;"
                    rules="rows" frame="hsides">
                    <tr>
                        <th>Order ID</th>
                        <th>Parent ID</th>
                        <th>Student ID</th>
                        <th>Meal ID</th>
                        <th>Date</th>
                        <th>Time</th>
                        <th>Total Amount</th>
                    </tr>
                    
                </table>
            </div>
        </div>

        <div class="card" style="width:800px;margin:auto;margin-top:30px;
            box-shadow:10px 10px 5px grey" align="center">
            <h5 class="card-header" style="background-color:#922B21">Meal Order Data</h5>
            <div class="card-body" style="background-color:#D98880">
                <table cellpadding="2px" align="center" style="text-align:center;"
                    rules="rows" frame="hsides">
                    <tr>
                        <th>Bill ID</th>
                        <th>Parent ID</th>
                        <th>Student ID</th>
                        <th>Date</th>
                        <th>Total Bill</th>
                    </tr>
                   
                </table>
            </div>
        </div>
    </body>
</html>


